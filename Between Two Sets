#include <iostream>
#include<bits/stdc++.h>
#include<algorithm>
using namespace std;

int main()
{
    int n, m,i,j,k,CountA,CountB,Answer,arrA[15],arrB[15];
    scanf("%d %d",&n,&m);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arrA[i]);
    }
    for(i=0;i<m;i++)
    {
        scanf("%d",&arrB[i]);
    }
    sort(arrA,arrA+n);
    sort(arrB,arrB+m);
    int start = arrA[n-1];
    int End = arrB[0];
    Answer=0;
    for(i=start;i<=End;i++)
    {
        CountA = 0;
        CountB = 0;
        for(j=0;j<=n-1;j++)
        {
            if(i%arrA[j] != 0)
                break;
            else
                CountA++;
        }
        if(CountA == n)
        {
            for(k=0;k<=m-1;k++)
            {
                if(arrB[k]%i != 0)
                    break;
                else
                    CountB++;
            }
            if(CountB == m)
                Answer++;
        }
    }
    printf("%d\n",Answer);

    return 0;
}
