#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x1,v1,x2,v2,flag=0,i;
    scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
    for(i=1;i<10;i++)
    {

    }
    if(v1>v2)
    {
        while(x1<x2)
        {
            x1+=v1;
            x2+=v2;
            if(x1==x2)
            {
                printf("YES\n");
                flag=1;
                break;
            }
        }
    }
    if(flag==0||v1<=v2)
        printf("NO\n");
    return 0;
}
